
 <div class="map-result col-md-4">map area
    <%- include ../shared/map.ejs %>
  </div>

  <div class="side-bar col-md-8"><h1>Results</h1>
    <% results.main.images.forEach(function(image){ %>
      <img src="<%= image.images.thumbnail.url  %>">
    <% }) %>

    <div class="section overview col-md-12">

      <div class="score">
        <form method="post" action="/favorites">
        <input type="hidden" name="fave_Address" value="<%= results.main.info.yelpZillow.zillow.address %>">
         <button class="btn btn-success">favorite</button>
        </form>
        <h2> Score:
        <%= results.main.info.yelpZillow.yelpFoodScore + results.main.info.yelpZillow.yelpEntertainmentScore %> </h2>
        <p>
          <%= results.main.info.community_snippet %>
        </p>

          <div class="progress">
            <div class="progress-bar progress-bar-success" role="progressbar" aria-valuenow="<%= (results.main.info.yelpZillow.yelpFoodScore + results.main.info.yelpZillow.yelpEntertainmentScore)%> " aria-valuemin="0" aria-valuemax="500" style="width: <%= 100 * (results.main.info.yelpZillow.yelpFoodScore + results.main.info.yelpZillow.yelpEntertainmentScore)/500%>%;">
              <span class="sr-only">500 Complete</span>
            </div>
          </div>
      </div>
    </div>

      <div class="overall-score col-md-12">
        <div class="col-md-3">
          <div class="walk-score small">
          Proximity: 95/100
          </div>
        </div>

        <div class="col-md-3">
          <div class="personlity small">
          People: Happy
          </div>
        </div>

        <div class="col-md-3">
          <div class="cost-of-living small ">
          Price: $$$$
          </div>
        </div>


        <div class="col-md-3">
          <div class="ratings sentiment small">
          Places: <%= ((results.main.info.yelpZillow.yelpFoodRating + results.main.info.yelpZillow.yelpEntertainmentRating)/2).toFixed(2) %> Stars
          </div>
        </div>
      </div>


      <div class="ratings-info">
      </div>
    </div>
  </div>